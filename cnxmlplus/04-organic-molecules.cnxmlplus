\psline(-0.6,1.3)(-0.6,1.9)
\rput(-0.6,2.1){\textbf{C}}
\psline(-0.8,2.1)(-1.4,2.1)
\rput(-1.6,2.1){\textbf{H}}
\psline(-0.4,2.1)(0.2,2.1)
\rput(0.4,2.1){\textbf{H}}
\psline(-0.6,2.3)(-0.6,2.9)
\rput(-0.6,3.1){\textbf{H}}
\psline(0,0)(0.6,0)
\rput(1,0){\textbf{C$_{(3)}$}}
\rput(0.8,1.1){\textbf{H}}
\psline(0.8,0.3)(0.8,0.9)
