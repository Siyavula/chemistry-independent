	      \psline(-3,0.2)(-3,0.8)
	      \rput(-3,1){\textbf{\green{\large X}}}
	      \psline(-3,-0.2)(-3,-0.8)
	      \rput(-3,-1){\textbf{H}}
	      \psline(-2.8,0)(-2.2,0)
	      \rput(-2,0){\textbf{H}}
	      \rput(-1,0){(b)}
	      \rput(0,0){\textbf{CH$_{3}${\green X}}}
	    </code>
	  </pspicture>
