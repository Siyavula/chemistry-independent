\rput(-2,0){\textbf{H}}
\rput(2,0){\textbf{H}}
\rput(-1,1){\textbf{H}}
\rput(-1,-1){\textbf{H}}
\rput(0,1){\textbf{H}}
\rput(1,1){\textbf{H}}
\rput(1,-1){\textbf{H}}
\rput(0,-1.5){\textbf{C}}
\rput(0,-2.5){\textbf{H}}
\rput(-1,-1.5){\textbf{H}}
\rput(1,-1.5){\textbf{H}}
\psline(-1.8,0)(-1.2,0)
\psline(-0.8,0)(-0.2,0)
\psline(0.8,0)(0.2,0)
\psline(1.8,0)(1.2,0)
\psline(-1,0.2)(-1,0.8)
\psline(-1,-0.2)(-1,-0.8)
\psline(0,0.2)(0,0.8)
