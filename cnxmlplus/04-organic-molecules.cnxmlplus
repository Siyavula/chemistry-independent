	      \psline[linecolor=orange](5.25,0.2)(5.25,0.6)
	      \rput(5.2,0.8){\textbf{\orange O}}
	      \psline[linecolor=orange](5.4,0)(5.8,0)
	      \rput(6.6,0){\textbf{\orange CH$_{2}$CH$_{3}$}}
	      \rput(7.7,0){\textbf{+}}
	      \rput(8.4,0){\textbf{H$_{2}$O}}
