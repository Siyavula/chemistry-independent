                    <code>
                      (-2.2,-2.7)(9.6,1.3)
		      \rput(0,0){
\rput(-2,0){\textbf{H}}
\psline(-1.8,0)(-1.2,0)
\rput(-0.8,0){\textbf{C$_{\red (1)}$}}
\psline(-1,0.3)(-1,0.9)
\psline(-1,-0.2)(-1,-0.8)
