		    \psline(2.2,0)(2.8,0)
		    \rput(3,0){\textbf{C}}
		    \psline(3.2,0)(3.8,0)
		    \rput(4,0){\textbf{C}}
		    \psline(4.2,0)(4.8,0)
		    \rput(5,0){\textbf{C}}
		    \newrgbcolor{orange}{1.0 0.35 0}
		    \rput(-1.9,-1.7){\textbf{\orange from alcohol}}
		    \rput(1.5,-1.7){\textbf{\red from carboxylic acid}}
		    </code>
		  </pspicture>
	      </workstep>
	      <workstep>
		<title>Combine this information to draw the structural representation for this molecule</title>
		  <pspicture>
		    <code>
		      (-4.2,-1.2)(6.2,1.2)
		     \rput(-4,0){\textbf{H}}
		     \psline(-3.8,0)(-3.2,0)
		      \rput(-3,0){\textbf{C}}
		      \psline(-3,0.2)(-3,0.8)
