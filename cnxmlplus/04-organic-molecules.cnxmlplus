		    \psline(-2,-0.2)(-2,-0.8)
		    \rput(-2,-1){\textbf{H}}
		    \psline(-1.8,0)(-1.2,0)
		    \rput(-1,0){\textbf{C}}
		    \psline(-1,0.2)(-1,0.8)
		    \rput(-1,1){\textbf{H}}
		    \psline(-1,-0.2)(-1,-0.8)
		    \rput(-1,-1){\textbf{H}}
